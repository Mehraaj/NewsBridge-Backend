generator client {
  provider        = "prisma-client-js"
  output          = "../generated/prisma"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["auth", "public"]
}

model article_entities {
  article_id String   @db.Uuid
  entity_id  BigInt
  articles   articles @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  entities   entities @relation(fields: [entity_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([article_id, entity_id])
  @@schema("public")
}

model articles {
  id                  String                    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  source              String?
  title               String?
  url                 String?                   @unique
  image_url           String?
  published_at        DateTime?                 @db.Timestamptz(6)
  category            String?
  author              String?
  content             String?
  summary             String?
  lat                 Float?
  lng                 Float?
  location_name       String?
  sentiment           sentiment_type?
  significance        String?
  created_at          DateTime?                 @default(now()) @db.Timestamptz(6)
  location            Unsupported("geography")?
  future_implications String?
  article_entities    article_entities[]
  article_tags        article_tags[]
  bookmarks           bookmarks[]
  geo_events          geo_events[]
  likes               likes[]
  views               views[]

  @@schema("public")
}

model entities {
  id               BigInt             @id @default(autoincrement())
  name             String             @unique
  type             String
  description      String?
  wiki_url         String?
  created_at       DateTime?          @default(now()) @db.Timestamptz(6)
  article_entities article_entities[]

  @@schema("public")
}

model user_article_views {
  id         BigInt    @id @default(autoincrement())
  user_id    String?   @db.Uuid
  article_id String?   @db.Uuid
  viewed_at  DateTime? @default(now()) @db.Timestamptz(6)
  liked      Boolean?  @default(false)
  bookmarked Boolean?  @default(false)

  @@schema("public")
}

model public_users {
  id               String             @id @db.Uuid
  name             String?            @unique
  email            String?
  password         String?
  preferences      Json?
  created_at       DateTime?          @default(now()) @db.Timestamptz(6)
  last_active      DateTime?          @db.Timestamptz(6)
  bookmark_folders bookmark_folders[]
  bookmarks        bookmarks[]
  likes            likes[]
  sessions         public_sessions?
  users            users              @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  views            views[]

  @@map("users")
  @@schema("public")
}

model weekly_forecasts {
  id           BigInt    @id @default(autoincrement())
  week_start   DateTime? @unique @db.Date
  generated_at DateTime? @default(now()) @db.Timestamptz(6)
  content      String?

  @@schema("public")
}

model article_tags {
  article_id String   @db.Uuid
  tag_id     BigInt
  articles   articles @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  tags       tags     @relation(fields: [tag_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([article_id, tag_id])
  @@schema("public")
}

model bookmark_folders {
  id          BigInt        @id @default(autoincrement())
  user_id     String?       @db.Uuid
  name        String?
  is_standard Boolean?      @default(false)
  created_at  DateTime?     @default(now()) @db.Timestamptz(6)
  users       public_users? @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  bookmarks   bookmarks[]

  @@unique([user_id, name])
  @@schema("public")
}

model bookmarks {
  id               BigInt            @id @default(autoincrement())
  user_id          String?           @db.Uuid
  article_id       String?           @db.Uuid
  folder_id        BigInt?
  bookmarked_at    DateTime?         @default(now()) @db.Timestamptz(6)
  articles         articles?         @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  bookmark_folders bookmark_folders? @relation(fields: [folder_id], references: [id], onUpdate: NoAction)
  users            public_users?     @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([user_id, article_id])
  @@schema("public")
}

model geo_events {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  article_id String?   @db.Uuid
  lat        Float?
  lng        Float?
  zoom_level Int?
  label      String?   @unique
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  articles   articles? @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([zoom_level, lat, lng])
  @@schema("public")
}

model likes {
  id         BigInt        @id @default(autoincrement())
  user_id    String?       @db.Uuid
  article_id String?       @db.Uuid
  liked_at   DateTime?     @default(now()) @db.Timestamptz(6)
  articles   articles?     @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users      public_users? @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([user_id, article_id])
  @@schema("public")
}

model tags {
  id           BigInt         @id @default(autoincrement())
  name         String?        @unique
  article_tags article_tags[]

  @@schema("public")
}

model views {
  id         BigInt        @id @default(autoincrement())
  user_id    String?       @db.Uuid
  article_id String?       @db.Uuid
  viewed_at  DateTime?     @default(now()) @db.Timestamptz(6)
  articles   articles?     @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users      public_users? @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@schema("public")
}

